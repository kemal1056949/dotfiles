[user]
    name = Kemal Kurniawan
[color]
    ui = true
[alias]
    co = checkout
    st = status -sb
    ds = diff --staged
    del = branch -d
    ren = branch -m
    graph = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset'
    standup = log --since '1 day ago' --oneline
    br = branch
    ci = commit
    llog = log --oneline -n 10
[core]
    editor = vim
    #pager = delta
[push]
    default = simple
[status]
    showUntrackedFiles = all
[pull]
    rebase = false
